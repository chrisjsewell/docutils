  </define>
  <define name="block_quote.attlist" combine="interleave">
    <ref name="basic.atts"/>
  </define>
  <define name="attribution">
    <element name="attribution">
      <ref name="attribution.attlist"/>
      <ref name="text.model"/>
    </element>
