<root>
   <test name="simple-variable">
      <string>x</string>
      <result>
         <math xmlns="http://www.w3.org/1998/Math/MathML">
            <mstyle>
               <mi>x</mi>
            </mstyle>
         </math>
      </result>
      <result>
         <math xmlns="http://www.w3.org/1998/Math/MathML"/>
      </result>
   </test>
   <test name="simple-number">
      <string>1</string>
      <result>
         <math xmlns="http://www.w3.org/1998/Math/MathML">
            <mstyle>
               <mn>1</mn>
            </mstyle>
         </math>
      </result>
   </test>
   <test name="simple-negative-number">
      <string>-1</string>
      <result>
         <math xmlns="http://www.w3.org/1998/Math/MathML">
            <mstyle>
               <mo>-</mo>
               <mn>1</mn>
            </mstyle>
         </math>
      </result>
   </test>
   <test name="simple-operator">
      <string>+</string>
      <result>
         <math xmlns="http://www.w3.org/1998/Math/MathML">
            <mstyle>
               <mo>+</mo>
            </mstyle>
         </math>
      </result>
   </test>
   <test name="simple-operator">
      <string>+</string>
      <result>
         <math xmlns="http://www.w3.org/1998/Math/MathML">
            <mstyle>
               <mo>+</mo>
            </mstyle>
         </math>
      </result>
   </test>
   <test name="simple-ascii-symbol">
      <string>alpha</string>
      <result>
         <math xmlns="http://www.w3.org/1998/Math/MathML">
            <mstyle>
               <mi>Î±</mi>
            </mstyle>
         </math>
      </result>
   </test>
   <test name="simple-addition">
      <string>1 + 1</string>
      <result>
         <math xmlns="http://www.w3.org/1998/Math/MathML">
            <mstyle>
               <mn>1</mn>
               <mo>+</mo>
               <mn>1</mn>
            </mstyle>
         </math>
      </result>
   </test>
   <test name="simple-fraction">
      <string>1/2</string>
      <result>
         <math xmlns="http://www.w3.org/1998/Math/MathML">
            <mstyle>
               <mfrac>
                  <mn>1</mn>
                  <mn>2</mn>
               </mfrac>
            </mstyle>
         </math>
      </result>
   </test>
   <test name="double-fraction">
      <string>1/2/3/4</string>
      <result>
         <math xmlns="http://www.w3.org/1998/Math/MathML">
            <mstyle>
               <mfrac>
                  <mfrac>
                     <mn>1</mn>
                     <mn>2</mn>
                  </mfrac>
                  <mfrac>
                     <mn>3</mn>
                     <mn>4</mn>
                  </mfrac>
               </mfrac>
            </mstyle>
         </math>
      </result>
      <result>
         <math  xmlns="http://www.w3.org/1998/Math/MathML">
            <mstyle >
               <mfrac>
                  <mn>1</mn>
                  <mn>2</mn>
               </mfrac>
               <mo>/</mo>
               <mfrac>
                  <mn>3</mn>
                  <mn>4</mn>
               </mfrac>
            </mstyle>
         </math>
      </result>
   </test>
   <test name="tripple-fraction">
      <string>1/2/3/4/5/6</string>
      <result>
         <math xmlns="http://www.w3.org/1998/Math/MathML">
            <mstyle>
               <mfrac>
                  <mfrac>
                     <mn>1</mn>
                     <mn>2</mn>
                  </mfrac>
                  <mfrac>
                     <mfrac>
                        <mn>3</mn>
                        <mn>4</mn>
                     </mfrac>
                     <mfrac>
                        <mn>5</mn>
                        <mn>6</mn>
                     </mfrac>
                  </mfrac>
               </mfrac>
            </mstyle>
         </math>
      </result>
   </test>
</root>
